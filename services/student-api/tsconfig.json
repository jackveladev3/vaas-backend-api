{
  "compilerOptions": {
    "lib": [
      "es2017",
      "dom"
    ],
    "removeComments": true,
    "module": "ES6",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "noUnusedParameters": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "target": "es2017",
    "declaration": false,
    "listFiles": false,
    "baseUrl": ".",
    "strict": true
  },
  "include": [
    "./src/**/*.ts",
    "src/login/getToken.js"
  ],
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
    ".vscode/**/*",
    "../../node_modules/**/*"
  ]
}